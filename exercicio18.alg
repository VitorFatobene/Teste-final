algoritmo "Nome do Programa"


var
n1, n2, n_n1, n_n2: inteiro

inicio
escreval("Digite um número inteiro: ")
leia(n1)
escreval("Digite outro número inteiro: ")
leia(n2)
escreval("a variável n1 = ",n1, " e a n2 = ",n2)
n_n1 <- n2
n_n2 <- n1
escreval("Com a permutação feita n1 = ", n_n1, " e n2 =  ", n_n2)
fimalgoritmo